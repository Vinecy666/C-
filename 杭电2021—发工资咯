#include <stdio.h>
int main() 
{
	int i,n,m;
	int k,b,c,d,e,f,sum;
	while(scanf("%d",&n)!=EOF&&n!=0)
	{   sum=0;
		for(i=0;i<n;i++)
		{   
			scanf("%d",&m);
		    k=m/100;  
            b=m%100/50;  
            c=m%100%50/10;  
            d=m%100%50%10/5;                 
            e=m%100%50%10%5/2;    
            f=m%100%50%10%5%2;
			sum=sum+k+b+c+d+e+f; 
		}
		
		printf("%d\n",sum);
	}
	return 0;
}
  
  
  
  
