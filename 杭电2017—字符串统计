#include <stdio.h>
#define N 1000
int main()
{
	char str[N];
	int i,j,n,num;
	while(scanf("%d",&n)!=EOF)
	{	
	  getchar();
	  for(j=0;j<n;j++)
	  {
         gets(str);
         num = 0;
         for(i = 0;str[i]!='\0';i++)
         {
    	   if((str[i]>='0')&&(str[i]<='9'))
    	   {
    		  num++;
		   }
	     }
        
       	printf("%d\n",num);
       }
    }
	return 0;
}
