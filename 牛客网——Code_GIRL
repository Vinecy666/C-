#include<stdio.h>
#include<string.h>
int main()
{
	char a[100];
	int n,i,j;
	scanf("%d",&n);
	i = 0;
	while(n)
	{
	
	   	if(n%2==0)
	  {
		 n = (n-2)/2;
		 a[i]='G';
		 i++;
	  }
	   else
	  {
	   	 n = (n-1)/2;
	   	 a[i]='N';
	   	 i++;
	  }
	} 
	for(j = i-1;j>=0;j--)
	  printf("%c",a[j]);
	printf("\n");
	
}
