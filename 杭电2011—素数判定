#include <stdio.h>
#include <math.h>
int main()
{
    int n,k;
    double j,s,i;
    scanf("%d",&n);
    while(n--)
    {
            scanf("%d",&k);
            s=0;
        for(i=1;i<=k;i++)
        {
         
          j=(1/i)*pow(-1,i+1);
          s=s+j;
        }
        printf("%.2f\n",s); 
    }
    return 0;
}
